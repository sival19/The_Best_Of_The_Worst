      <TextField fx:id="personID" layoutX="518.0" layoutY="159.0" prefHeight="26.0" prefWidth="47.0">
         <effect>
            <InnerShadow />
         </effect></TextField>
      <TextArea fx:id="beskrivelse" layoutX="660.0" layoutY="144.0" prefHeight="200.0" prefWidth="200.0">
         <effect>
            <InnerShadow />
         </effect></TextArea>
      <Button fx:id="tilbage" layoutX="14.0" layoutY="54.0" mnemonicParsing="false" onAction="#toStartScreen" text="Tilbage">
